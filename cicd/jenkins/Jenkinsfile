pipeline {
    agent any

    stages {
        stage('Init') {
            steps {
                echo 'Running ${env.BUILD_ID} on ${env.JENKINS_URL}'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}